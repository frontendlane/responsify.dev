{
	"name": "responsify-dev",
	"version": "5.0.0",
	"author": {
		"name": "Petar Petrovic",
		"email": "petar@frontendlane.com",
		"url": "https://frontendlane.com/about/"
	},
	"homepage": "https://responsify.dev",
	"repository": "https://github.com/frontendlane/responsify/",
	"bugs": "https://github.com/frontendlane/responsify/issues",
	"license": "ISC",
	"keywords": [
		"responsive",
		"css",
		"scss",
		"calc",
		"@media"
	],
	"description": "Responsify: Prevent abrupt layout shift when users resize their browsers",
	"scripts": {
		"update": "npm install && npx npm-check-updates -u && rm -rf node_modules && rm package-lock.json && npm install",
		"dev": "npm run zip && ./shell-scripts/dev.sh",
		"build": "npm run type-check && next build",
		"zip": "zip public/_responsify.scss.zip public/_responsify.scss",
		"test": "vitest",
		"type-check": "tsc",
		"start": "next start",
		"format": "prettier --write .",
		"lint": "next lint"
	},
	"dependencies": {
		"@hookform/resolvers": "3.9.1",
		"clipboard": "2.0.11",
		"next": "15.0.2",
		"react": "19.0.0-rc-02c0e824-20241028",
		"react-dom": "19.0.0-rc-02c0e824-20241028",
		"react-hook-form": "7.53.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@testing-library/dom": "10.4.0",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "16.0.1",
		"@types/node": "22",
		"@types/react": "18",
		"@types/react-dom": "18",
		"@vitejs/plugin-react": "4.3.3",
		"concurrently": "9.1.0",
		"eslint": "9",
		"eslint-config-next": "15.0.2",
		"happy-css-modules": "3.2.0",
		"jsdom": "25.0.1",
		"npm-check-updates": "17.1.10",
		"prettier": "3.3.3",
		"typescript": "5",
		"vitest": "2.1.4"
	},
	"overrides": {
		"react": "19.0.0-rc-02c0e824-20241028",
		"react-dom": "19.0.0-rc-02c0e824-20241028"
	},
	"private": true
}
